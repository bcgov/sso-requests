{
  "name": "sso-requests-localserver",
  "version": "0.1.0",
  "description": "Express server for running lambda functions in local dev environment",
  "license": "Apache-2.0",
  "author": "SSO Team",
  "scripts": {
    "dev": "nodemon --trace-warnings",
    "migrate-db": "nodemon --config nodemon-db.json"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.41",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "chalk": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.16",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.8.2"
  },
  "resolutions": {
    "minimist": ">=1.2.6"
  },
  "nodemonConfig": {
    "watch": [
      "./",
      "../lambda"
    ],
    "ext": "ts,json",
    "exec": "ts-node -r tsconfig-paths/register -r dotenv/config ./server.ts"
  },
  "dependencies": {
    "cors": "^2.8.5"
  }
}
